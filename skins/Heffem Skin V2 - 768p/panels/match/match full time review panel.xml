              <!-- Shot gone in -->
              <integer value="1" />
              <!-- Shot missed target -->
              <integer value="2" />
              <!-- Shot hit woodwork -->
              <integer value="3" />
              <!-- Shot saved -->
              <integer value="4" />
              <!-- Shot blocked -->
              <integer value="5" />
              <!-- Foul against -->
