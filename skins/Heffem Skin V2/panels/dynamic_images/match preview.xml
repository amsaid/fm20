<?xml version="1.0" encoding="UTF-8"?>
<panel>
    <layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true" />
    <container class="client_object_viewer_xml_panel">
        <layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true" />
        <record id="object_property" get_property="Ttea" set_property="objt" />
        <widget class="picture" file="heffem/dynamics/bg" image_alignment="centre" scale_picture="true" keep_aspect_ratio="false">
            <record id="object_property" get_property="oPCn" set_property="rrep" />
        </widget>
		<widget class="picture" file="backgrounds/pattern home left" image_alignment="left" scale_picture="true" keep_aspect_ratio="true">
			<record id="object_property" get_property="oSCn" set_property="rrep" dont_set_object="true"/>
		</widget>
    </container>
    <container class="client_object_viewer_xml_panel">
        <layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true" />
        <record id="object_property" get_property="ottm" set_property="objt" />
        <widget class="picture" file="heffem/dynamics/match report left" image_alignment="centre" scale_picture="true" keep_aspect_ratio="false">
            <record id="object_property" get_property="oPCn" set_property="rrep" />
        </widget>
		<widget class="picture" file="backgrounds/pattern home right" image_alignment="right" scale_picture="true" keep_aspect_ratio="true">
			<record id="object_property" get_property="oSCn" set_property="rrep" dont_set_object="true" />
		</widget>
    </container>
    <widget class="picture" id="bgim" file="heffem/dynamics/match report centre" scale_picture="true" keep_aspect_ratio="false" />
    <container>
        <layout class="arrange_vertical_attachment" layout="-1,-1,-1" offset="0" gap="0" />
        <layout class="stick_to_sides_attachment" alignment="horizontal" inset="0" apply_to_children="true" />
        <widget class="spacer" />
        <container>
            <layout class="arrange_horizontal_attachment" layout="-2,-5,-1,-5,-1,-5,-2" offset="0" gap="0" />
            <layout class="stick_to_sides_attachment" alignment="vertical" inset="0" apply_to_children="true" />
            <widget class="spacer" />
            <!-- Logo/Portrait -->
            <widget class="spacer" />
            <widget class="spacer" />
            <widget class="text" alignment="centre,auto_fit" colour="heffem" font="attributs2" value="V">
                <colour id="link_colour" red="0" green="0" blue="0" alpha="1" />
            </widget>
            <widget class="spacer" />
            <!-- Logo/Portrait -->
            <widget class="spacer" />
            <widget class="spacer" />
        </container>
        <widget class="spacer" />
    </container>
	<container class="horizontal_adaptive_container" layout="-1,-3,-6,-3,-1" inset="0" offset="0" gap="0">
        <container />
            <widget class="object_portrait_picture" id="log1" image_alignment="all" scale_picture="true" keep_aspect_ratio="true" wants_mouse_events="false">
                <record id="object_property" get_property="Ttea" set_property="objt" />
            </widget>
        <container />
            <widget class="object_portrait_picture" id="log2" image_alignment="all" scale_picture="true" keep_aspect_ratio="true" wants_mouse_events="false">
                <record id="object_property" get_property="ottm" set_property="objt" />
            </widget>
        <container />
    </container>
</panel>